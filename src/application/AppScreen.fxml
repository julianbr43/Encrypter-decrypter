<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="294.0" prefWidth="336.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AppScreenController">
   <children>
      <Button fx:id="encrytper" layoutX="175.0" layoutY="100.0" mnemonicParsing="false" onAction="#encrypter" prefWidth="122.0" text="Encrypter">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Button fx:id="decrypter" layoutX="175.0" layoutY="135.0" mnemonicParsing="false" onAction="#decrypter" prefHeight="25.0" prefWidth="122.0" text="Decrypter">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font></Button>
      <Label alignment="CENTER" layoutX="48.0" layoutY="29.0" prefHeight="28.0" prefWidth="255.0" text="Encrypter-Decrypter">
         <font>
            <Font name="Arial Rounded MT Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="filechoose" layoutX="77.0" layoutY="112.0" mnemonicParsing="false" onAction="#choosefile" text="Choose file">
         <font>
            <Font name="Arial Rounded MT Bold" size="12.0" />
         </font>
      </Button>
      <TextField fx:id="password" layoutX="132.0" layoutY="200.0" />
      <Label layoutX="68.0" layoutY="204.0" text="Password" />
      <Label fx:id="path" layoutX="77.0" layoutY="147.0" prefHeight="17.0" prefWidth="81.0" />
      <Label layoutX="40.0" layoutY="147.0" text="File:" />
   </children>
</Pane>
